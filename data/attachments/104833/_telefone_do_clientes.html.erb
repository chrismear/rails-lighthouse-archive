<% telefone_do_clientes.lista.each do |telefone_do_cliente| %>
  <% form_remote_tag :url => { :action => :remove_telefone, :telefone => telefone_do_cliente.telefone } do %>
     <%=h telefone_do_cliente.tipo_de_telefone.nome %> : <%=h telefone_do_cliente.telefone %>
    <% if controller.action_name != 'show' %>
      <%= image_submit_tag '/images/icon_remove.png' %>
    <% end %>
  <% end %>
  <br />
<% end %>

<% if telefone_do_clientes.lista.empty? %>
  <br />
<% end %>