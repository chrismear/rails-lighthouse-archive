  match 'test/:param1/:param2' =&gt; 'welcome#index', :as =&gt; 'random_welcome'
end
@@@

@@@
# script/rails c
Loading development environment (Rails 3.0.0.rc)
&gt;&gt; app.random_welcome_url(:param1 =&gt; 'test', :param2 =&gt; 'test')
=&gt; &quot;http://www.example.com/test/test/test&quot;
@@@

